continue
-----------------

public class ContinueExample {  
public static void main(String[] args) {  
    for(int i=1;i<=10;i++){  
        if(i==5){  
            continue;  //what happened ?? simply skipped i=5 and proceed the rest
        }  
        System.out.println(i);    o/p :- 1 to 10 except 5
    }  
}  
}  
